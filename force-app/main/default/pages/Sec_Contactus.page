   <script>
    //console.log('sanity check');
        window.onload = function() {
             // Send the size of the form on page load
    // Because this is async we'll request that the parent acknowledge the message and
    // we'll re-send the message every 2 seconds until we get confirmation
    var formHeight = document.getElementsByClassName('hs-form')[0].scrollHeight;
    var contactFormAcknowledgmentInterval;
    var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
    var eventer = window[eventMethod];
    var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";

    // receipt messages from the parent
    eventer(messageEvent,function(e) {
        //console.log("Resource form iframe listener got a message: ");
        //console.log(e.data);

        // Check that message being passed concerns the resource form
        if ((typeof (e.data) == 'object') && e.data.contactForm) {
            // If its a receipt message
            if ( e.data.contactForm.receipt) {
                //console.log("It's a receipt! For the contactForm!");
                clearInterval(contactFormAcknowledgmentInterval);
            }
        }

    });

    message = {
        contactForm: {
            height: formHeight,
            acknowledgeReceipt: true
        }
    };

    //console.log('Resource Form sending initial size message');
    parent.postMessage(message, '*');

    contactFormAcknowledgmentInterval = setTimeout(function() {
        //console.log('Resource Form sending size message again...');
        parent.postMessage(message, '*');
    }, 2000);


    // Alert parent of re-size
    window.onresize = function(event) {
        var newFormHeight = document.getElementsByClassName('hs-form')[0].scrollHeight;
        var heightDiff = formHeight - newFormHeight;
        if ( heightDiff > 10 || heightDiff < -10 ) {
            formHeight = newFormHeight;

            message = {
                contactForm: {
                    height: formHeight
                }
            };

            parent.postMessage(message,"*");
        }
    }
        }
        

        
    </script>
    
